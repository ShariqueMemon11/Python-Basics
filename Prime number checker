def prime_num():
    num = int(input("enter num between 1 to 1000 to check that num is prime or not "))
    while(num > 1000):
        num = int(input("enter num between 1 to 1000 to check that num is prime or not "))
    for i in range(2,num):
        if num % i == 0:
            return True;
            break
    
    else:
        return False;


check = prime_num(); 

if check == True:
    print("it is not a prime ")
else:
    print("it is a prime ")
